{
  "extends": "@react-native/typescript-config/tsconfig.json",
  "exclude": ["./flipper-debugger"],
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"],
      "@assets/*": ["./src/assets/*"],
      "@components/*": ["./src/components/*"],
      "@screens/*": ["./src/screens/*"],
      "@store/*": ["./src/store/*"],
      "@util/*": ["./src/util/*"],
      "@hooks/*": ["./src/hooks/*"],
      "@animation/*": ["./src/animation/*"],
      "@myTypes/*": ["./src/types/*"],
      "@baseApi": ["./src/baseApi/baseApi"],
      "@constant/*": ["./src/constant/*"]
    }
  }
}
